{
  "name": "sphinx-myst-demo",
  "version": "1.0.0",
  "scripts": {
    "sphinx:html:serve": "serve ./_build/sphinx-html -l 8080",
    "sphinx:html:build": "sphinx-build -b html sphinx _build/sphinx-html",
    "sphinx:myst:serve": "serve ./_build/sphinx -l 9090",
    "sphinx:myst:build": "sphinx-build -b myst sphinx _build/sphinx",
    "sphinx:watch": "npm-watch sphinx:build",
    "sphinx:build": "npm-run-all -s sphinx:myst:build sphinx:html:build",
    "sphinx": "npm-run-all -p sphinx:watch sphinx:myst:serve sphinx:html:serve",
    "myst:build": "npm-run-all -s myst:clean myst:start",
    "myst:clean": "myst clean --cache -y",
    "myst:start": "myst start --headless",
    "myst": "npm-watch myst:build"
  },
  "watch": {
    "sphinx:build": {
      "patterns": [
        "**"
      ],
      "extensions": [
        "py",
        "rst"
      ],
      "runOnChangeOnly": false
    },
    "myst:build": {
      "patterns": [
        "_build/sphinx"
      ],
      "extensions": [
        "json"
      ],
      "runOnChangeOnly": false
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "serve": "^14.0.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.13.0",
    "mystmd": "^1.2.9"
  }
}
